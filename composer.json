{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name"    : "twitter/bootstrap",
                "version" : "2.1.1",
                "source"  : {
                    "url"       : "https://github.com/twitter/bootstrap.git",
                    "type"      : "git",
                    "reference" : "v2.1.1"
                }
            }
        }
    ],
    "require": {
        "php"                                   : ">=5.3.3",
        "umpirsky/sylex"                        : "*",
        "twig/twig"                             : "1.*",
        "monolog/monolog"                       : "1.0.*",
        "symfony/form"                          : "2.1.*",
        "symfony/translation"                   : "2.1.*",
        "symfony/twig-bridge"                   : "2.1.*",
        "symfony/validator"                     : "2.1.*",
        "symfony/yaml"                          : "2.1.*",
        "symfony/config"                        : "2.1.*",
        "symfony/console"                       : "2.1.*",
        "symfony/security"                      : "2.1.*",
        "symfony/doctrine-bridge"               : "2.1.*",
        "kriswallsmith/assetic"                 : "1.0.*",
        "twitter/bootstrap"                     : "2.1.1",
        "mheap/silex-assetic"                   : "*",
        "palma/DoctrineORMServiceProvider"      : "*",
        "umpirsky/i18n-routing-service-provider": "*",
        "gedmo/doctrine-extensions"             : "2.3.*",
        "bcc/extra-tools-bundle"                : "*"
    },
    "require-dev": {
        "symfony/dom-crawler"                   : "2.1.*",
        "symfony/css-selector"                  : "2.1.*",
        "symfony/browser-kit"                   : "2.1.*",
        "nelmio/alice"                          : "*",
        "umpirsky/twig-gettext-extractor"       : "*"
    },
    "minimum-stability" : "dev",
    "scripts": {
        "post-install-cmd": "Sos\\Composer\\Script::postInstall"
    },
    "autoload": {
        "psr-0": {
            "Sos": "src/"
        }
    }
}
